{
  "version": 2,
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.js",
      "use": "@vercel/static"
    },
    {
      "src": "assets/**",
      "use": "@vercel/static"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/overview",
      "dest": "/overview.html"
    },
    {
      "src": "/schedule",
      "dest": "/schedule.html"
    },
    {
      "src": "/sendrequest",
      "dest": "/sendrequest.html"
    },
    {
      "src": "/notification",
      "dest": "/notification.html"
    },
    {
      "src": "/overview.html",
      "dest": "/overview.html"
    },
    {
      "src": "/schedule.html",
      "dest": "/schedule.html"
    },
    {
      "src": "/sendrequest.html",
      "dest": "/sendrequest.html"
    },
    {
      "src": "/notification.html",
      "dest": "/notification.html"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/js/$1"
    },
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}